            "display_name": "Get Inbox",
            "description": "Get details of a specific inbox",
            "input_schema": {
                "type": "object",
                "properties": {
                    "inbox_id": {
                        "type": "string",
                        "description": "The inbox ID to retrieve"
                    }
                },
                "required": ["inbox_id"]
            },
            "output_schema": {
                "type": "object",
                "properties": {
                    "inbox": {
                        "type": "object",
                        "description": "Inbox details",
                        "properties": {
                            "id": {
                                "type": "string",
                                "description": "Inbox ID"
                            },
                            "name": {
                                "type": "string",
                                "description": "Inbox name"
                            },
                            "address": {
                                "type": "string",
                                "description": "Inbox email address"
                            },
                            "type": {
                                "type": "string",
                                "description": "Inbox type"
                            },
                            "send_as": {
                                "type": "string",
                                "description": "Send as email address"
                            },
                            "is_private": {
                                "type": "boolean",
                                "description": "Whether inbox is private"
                            }
                        },
                        "required": ["id", "name", "address"]
                    },
                    "result": {
                        "type": "boolean",
                        "description": "Whether the operation was successful"
                    },
                    "error": {
                        "type": "string",
                        "description": "Error message if the action failed"
                    }
                },
                "required": ["inbox", "result"]
            }
        },
        "list_inbox_conversations": {
            "display_name": "List Inbox Conversations",
            "description": "List conversations from a specific inbox",
            "input_schema": {
                "type": "object",
                "properties": {
                    "inbox_id": {
                        "type": "string",
                        "description": "The inbox ID to get conversations from"
                    },
                    "status": {
                        "type": "string",
                        "description": "Filter by conversation status",
                        "enum": ["archived", "deleted", "open", "assigned", "unassigned"]
                    },
                    "tag_id": {
                        "type": "string",
                        "description": "Filter by tag ID (optional)"
                    },
                    "limit": {
                        "type": "integer",
                        "description": "Maximum number of conversations to return (default: 50)",
                        "default": 50
                    }
                },
                "required": ["inbox_id"]
            },
            "output_schema": {
                "type": "object",
                "properties": {
                    "conversations": {
                        "type": "array",
                        "description": "List of conversations",
                        "items": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "description": "Conversation ID"
                                },
                                "subject": {
                                    "type": "string",
                                    "description": "Conversation subject"
                                },
                                "status": {
                                    "type": "string",
                                    "description": "Conversation status"
                                },
                                "assignee": {
                                    "type": "object",
                                    "description": "Assigned teammate"
                                },
                                "recipient": {
                                    "type": "object",
                                    "description": "Conversation recipient"
                                },
                                "tags": {
                                    "type": "array",
                                    "description": "Conversation tags"
                                },
                                "last_message": {
                                    "type": "object",
                                    "description": "Last message in conversation"
                                }
                            },
                            "required": ["id", "subject", "status"]
                        }
                    },
                    "result": {
                        "type": "boolean",
                        "description": "Whether the operation was successful"
                    },
                    "error": {
                        "type": "string",
                        "description": "Error message if the action failed"
                    }
                },
                "required": ["conversations", "result"]
            }
        },
