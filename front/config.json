                "type": "object",
                "properties": {
                    "channel_id": {
                        "type": "string",
                        "description": "Channel ID to send from"
                    },
                    "body": {
                        "type": "string",
                        "description": "Message body content"
                    },
                    "to": {
                        "type": "array",
                        "description": "List of recipient email addresses",
                        "items": {
                            "type": "string"
                        }
                    },
                    "cc": {
                        "type": "array",
                        "description": "List of CC email addresses",
                        "items": {
                            "type": "string"
                        }
                    },
                    "bcc": {
                        "type": "array",
                        "description": "List of BCC email addresses",
                        "items": {
                            "type": "string"
                        }
                    },
                    "sender_name": {
                        "type": "string",
                        "description": "Name used for the sender info of the message"
                    },
                    "subject": {
                        "type": "string",
                        "description": "Message subject for email messages"
                    },
                    "author_id": {
                        "type": "string",
                        "description": "ID of the teammate on behalf of whom the message is sent"
                    },
                    "text": {
                        "type": "string",
                        "description": "Text version of the body for email messages"
                    },
                    "signature_id": {
                        "type": "string",
                        "description": "ID of the signature to attach to this message"
                    },
                    "should_add_default_signature": {
                        "type": "boolean",
                        "description": "Whether to add the default signature"
                    }
                },
                "required": ["channel_id", "body", "to"]
            },
            "output_schema": {
                "type": "object",
                "properties": {
                    "message_uid": {
                        "type": "string",
                        "description": "Temporary message UID (async processing)"
