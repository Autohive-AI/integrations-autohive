                        "type": "string",
                        "description": "Updated event name"
                    },
                    "summary": {
                        "type": "string",
                        "description": "Updated event summary"
                    },
                    "start_utc": {
                        "type": "string",
                        "description": "Updated start date/time in UTC"
                    },
                    "end_utc": {
                        "type": "string",
                        "description": "Updated end date/time in UTC"
                    },
                    "timezone": {
                        "type": "string",
                        "description": "Updated timezone"
                    },
                    "currency": {
                        "type": "string",
                        "description": "Updated currency"
                    },
                    "online_event": {
                        "type": "boolean",
                        "description": "Whether this is an online event"
                    },
                    "venue_id": {
                        "type": "string",
                        "description": "Updated venue ID"
                    },
                    "listed": {
                        "type": "boolean",
                        "description": "Whether the event is publicly listed"
                    },
                    "capacity": {
                        "type": "integer",
                        "description": "Updated capacity"
                    }
                },
                "required": ["event_id"]
            },
            "output_schema": {
                "type": "object",
                "properties": {
                    "event": {
                        "type": "object",
                        "description": "Updated event details"
                    },
                    "result": {
                        "type": "boolean",
                        "description": "Whether the operation was successful"
                    },
                    "error": {
                        "type": "string",
                        "description": "Error message if the action failed"
                    }
                },
                "required": ["event", "result"]
            }
        },
        "delete_event": {
            "display_name": "Delete Event",
            "description": "Deletes an event. Event must not have any pending or completed orders.",
