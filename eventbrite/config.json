{
    "name": "Eventbrite",
    "display_name": "Eventbrite",
    "version": "1.0.0",
    "description": "Eventbrite integration for managing events, attendees, orders, venues, and ticket classes",
    "entry_point": "eventbrite.py",
    "auth": {
        "type": "platform",
        "provider": "Eventbrite",
        "scopes": [
            "event.read",
            "event.write",
            "order.read",
            "attendee.read",
            "venue.read",
            "venue.write",
            "organization.read",
            "user.read"
        ]
    },
    "actions": {
        "get_current_user": {
            "display_name": "Get Current User",
            "description": "Retrieves information about the currently authenticated user.",
            "input_schema": {
                "type": "object",
                "properties": {},
                "required": []
            },
            "output_schema": {
                "type": "object",
                "properties": {
                    "user": {
                        "type": "object",
                        "description": "User details"
                    },
                    "result": {
                        "type": "boolean",
                        "description": "Whether the operation was successful"
                    },
                    "error": {
                        "type": "string",
                        "description": "Error message if the action failed"
                    }
                },
                "required": ["result"]
            }
        },
        "list_organizations": {
            "display_name": "List Organizations",
            "description": "Lists all organizations the authenticated user is a member of.",
            "input_schema": {
                "type": "object",
                "properties": {},
                "required": []
            },
            "output_schema": {
                "type": "object",
                "properties": {
                    "organizations": {
                        "type": "array",
                        "description": "List of organizations"
                    },
                    "result": {
                        "type": "boolean",
                        "description": "Whether the operation was successful"
                    },
                    "error": {
                        "type": "string",
                        "description": "Error message if the action failed"
                    }
                },
                "required": ["organizations", "result"]
            }
        },
        "get_event": {
            "display_name": "Get Event",
            "description": "Retrieves details of a specific event by its ID.",
            "input_schema": {
                "type": "object",
                "properties": {
                    "event_id": {
                        "type": "string",
                        "description": "The ID of the event to retrieve"
                    },
                    "expand": {
                        "type": "array",
                        "description": "Optional expansions to include (e.g., ['venue', 'organizer', 'category'])",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "required": ["event_id"]
            },
            "output_schema": {
                "type": "object",
                "properties": {
                    "event": {
                        "type": "object",
                        "description": "Event details"
                    },
                    "result": {
                        "type": "boolean",
                        "description": "Whether the operation was successful"
                    },
                    "error": {
                        "type": "string",
                        "description": "Error message if the action failed"
                    }
                },
                "required": ["event", "result"]
            }
        },
        "list_events": {
            "display_name": "List Events",
            "description": "Lists events for an organization.",
            "input_schema": {
                "type": "object",
                "properties": {
                    "organization_id": {
                        "type": "string",
                        "description": "Organization ID to list events from"
                    },
                    "status": {
                        "type": "string",
                        "description": "Filter by event status (draft, live, started, ended, completed, canceled, all)",
                        "enum": ["draft", "live", "started", "ended", "completed", "canceled", "all"]
                    },
                    "order_by": {
                        "type": "string",
                        "description": "Sort order (start_asc, start_desc, created_asc, created_desc, name_asc, name_desc)",
                        "enum": ["start_asc", "start_desc", "created_asc", "created_desc", "name_asc", "name_desc"]
                    },
                    "time_filter": {
                        "type": "string",
                        "description": "Filter by time (all, past, current_future)",
                        "enum": ["all", "past", "current_future"]
                    },
                    "page_size": {
                        "type": "integer",
                        "description": "Number of results per page (max 50)"
                    }
                },
                "required": ["organization_id"]
            },
            "output_schema": {
                "type": "object",
                "properties": {
                    "events": {
                        "type": "array",
                        "description": "List of events"
                    },
                    "pagination": {
                        "type": "object",
                        "description": "Pagination information"
                    },
                    "result": {
                        "type": "boolean",
                        "description": "Whether the operation was successful"
                    },
                    "error": {
                        "type": "string",
                        "description": "Error message if the action failed"
                    }
                },
                "required": ["events", "result"]
            }
        },
        "create_event": {
            "display_name": "Create Event",
            "description": "Creates a new event in an organization.",
            "input_schema": {
                "type": "object",
                "properties": {
                    "organization_id": {
                        "type": "string",
                        "description": "Organization ID where the event will be created"
                    },
                    "name": {
                        "type": "string",
                        "description": "Event name/title"
                    },
                    "summary": {
                        "type": "string",
                        "description": "Short summary describing the event (max 140 chars)"
                    },
                    "start_utc": {
                        "type": "string",
                        "description": "Event start date/time in UTC (ISO 8601 format, e.g., 2024-12-25T18:00:00Z)"
                    },
                    "end_utc": {
                        "type": "string",
                        "description": "Event end date/time in UTC (ISO 8601 format)"
                    },
                    "timezone": {
                        "type": "string",
                        "description": "Timezone for the event (e.g., America/Los_Angeles)"
                    },
                    "currency": {
                        "type": "string",
                        "description": "Event currency (ISO 4217 code, e.g., USD)"
                    },
                    "online_event": {
                        "type": "boolean",
                        "description": "Whether this is an online-only event"
                    },
                    "venue_id": {
                        "type": "string",
                        "description": "Venue ID for in-person events"
                    },
                    "organizer_id": {
                        "type": "string",
                        "description": "Organizer ID"
                    },
                    "category_id": {
                        "type": "string",
                        "description": "Category ID for the event"
                    },
                    "listed": {
                        "type": "boolean",
                        "description": "Whether the event is publicly listed"
                    },
                    "shareable": {
                        "type": "boolean",
                        "description": "Whether the event is shareable"
                    },
                    "capacity": {
                        "type": "integer",
                        "description": "Maximum event capacity"
                    }
                },
                "required": ["organization_id", "name", "start_utc", "end_utc", "timezone", "currency"]
            },
            "output_schema": {
                "type": "object",
                "properties": {
                    "event": {
                        "type": "object",
                        "description": "Created event details"
                    },
                    "result": {
                        "type": "boolean",
                        "description": "Whether the operation was successful"
                    },
                    "error": {
                        "type": "string",
                        "description": "Error message if the action failed"
                    }
                },
                "required": ["event", "result"]
            }
        },
        "update_event": {
            "display_name": "Update Event",
